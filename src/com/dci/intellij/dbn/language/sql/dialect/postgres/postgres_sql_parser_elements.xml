<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE element-defs SYSTEM "../../../common/definition/language-parser-elements.dtd">--><element-defs language="POSTGRES-SQL" version="9.3">
    <element-def attributes="ROOT, STRUCTURE" description="SQL block" icon="FILE_BLOCK_SQL" id="sql_block">
        <iteration id="000000000">
            <one-of attributes="STATEMENT, SCOPE_ISOLATION" formatting-indent="ABSOLUTE_NONE" id="000000001">
                <element ref-id="insert_statement" />
                <element ref-id="update_statement" />
                <element ref-id="delete_statement" />
                <element ref-id="truncate_statement" />
                <element ref-id="select_statement" />
                <element optional="true" ref-id="with_clause" />
            </one-of>
        </iteration>
    </element-def>


    <!-- ========= TRUNCATE ========= -->
    <element-def attributes="EXECUTABLE, DATA_MANIPULATION, TRANSACTIONAL, STRUCTURE" description="TRUNCATE statement" id="truncate_statement">
        <token id="000000002" type-id="KW_TRUNCATE" />
        <iteration separator="CHR_COMMA" id="000000003">
            <sequence id="000000004">
                <token id="000000005" optional="true" type-id="KW_TABLE" />
                <token id="000000006" optional="true" type-id="KW_ONLY" />
                <qualified-identifier id="000000007" separator="CHR_DOT">
                    <variant>
                        <object-ref id="000000008" optional="true" referenceable="true" type="SCHEMA" />
                        <object-ref id="000000009" referenceable="true" type="DATASET" />
                    </variant>
                </qualified-identifier>
                <token optional="true" type-id="CHR_STAR" id="000000010" />
            </sequence>
        </iteration>
        <sequence optional="true" id="000000011">
            <one-of id="000000012">
                <token id="000000013" type-id="KW_RESTART" />
                <token id="000000014" type-id="KW_CONTINUE" />
            </one-of>
            <token id="000000015" type-id="KW_IDENTITY" />
        </sequence>
        <one-of optional="true" id="000000016">
            <token id="000000017" type-id="KW_CASCADE" />
            <token id="000000018" type-id="KW_RESTRICT" />
        </one-of>
    </element-def>

    <!-- ========= DELETE ========= -->
    <element-def attributes="EXECUTABLE, DATA_MANIPULATION, TRANSACTIONAL, STRUCTURE" description="DELETE statement" id="delete_statement">
        <element optional="true" ref-id="with_clause" />
        <token exit="true" id="000000019" type-id="KW_DELETE" />
        <token id="000000020" type-id="KW_FROM" />
        <token id="000000021" optional="true" type-id="KW_ONLY" />
        <qualified-identifier id="000000022" separator="CHR_DOT">
            <variant>
                <object-ref id="000000023" optional="true" referenceable="true" type="SCHEMA" />
                <object-ref id="000000024" referenceable="true" type="DATASET" />
            </variant>
        </qualified-identifier>
        <token optional="true" type-id="CHR_STAR" id="000000025" />
        <sequence optional="true" id="000000026">
            <token id="000000027" optional="true" type-id="KW_AS" />
            <alias-def id="000000028" type="DATASET" />
        </sequence>
        <sequence optional="true" id="000000029">
            <token type-id="KW_USING" id="000000030" />
            <element ref-id="expression_list" />
        </sequence>
        <sequence optional="true" id="000000031">
            <token id="000000032" type-id="KW_WHERE" />
            <one-of id="000000033">
                <sequence id="000000034">
                    <token type-id="KW_CURRENT" id="000000035" />
                    <token type-id="KW_OF" id="000000036" />
                    <object-ref original-name="cursor_name" type="CURSOR" id="000000037" />
                </sequence>
                <element ref-id="condition" />
            </one-of>
        </sequence>
        <sequence optional="true" id="000000038">
            <token id="000000039" type-id="KW_RETURNING" />
            <iteration separator="CHR_COMMA" id="000000040">
                <sequence id="000000041">
                    <element ref-id="expression" />
                    <sequence optional="true" id="000000042">
                        <token type-id="KW_AS" id="000000043" />
                        <alias-def type="ANY" id="000000044" />
                    </sequence>
                </sequence>
            </iteration>
        </sequence>
    </element-def>

    <!-- ========= select ========= -->
    <element-def attributes="EXECUTABLE, DATA_MANIPULATION, QUERY, STRUCTURE" description="SELECT statement" id="select_statement">
        <element ref-id="subquery" />
        <element ref-id="end" />
    </element-def>
    <element-def attributes="EXECUTABLE, DATA_MANIPULATION, QUERY" description="Subquery" id="subquery" virtual-object="DATASET">
        <one-of id="000000045">
            <sequence id="000000046">
                <element optional="true" ref-id="with_clause" />
                <token exit="true" id="000000047" type-id="KW_SELECT" />
                <element ref-id="select_list" />
                <element optional="true" ref-id="from_clause" />
                <element optional="true" ref-id="where_clause" />
                <element optional="true" ref-id="group_by_clause" />
                <element optional="true" ref-id="having_clause" />
                <element optional="true" ref-id="window_clause" />
                <element optional="true" ref-id="union_intersect_except_clause" />
                <element optional="true" ref-id="order_by_clause" />
                <element optional="true" ref-id="limit_clause" />
                <element optional="true" ref-id="offset_clause" />
                <element optional="true" ref-id="fetch_clause" />
                <element optional="true" ref-id="for_clause" />
            </sequence>
            <wrapper id="000000048" template="PARENTHESES">
                <element ref-id="subquery" />
            </wrapper>
        </one-of>
        <element optional="true" ref-id="order_by_clause" />
    </element-def>
    <element-def attributes="CLAUSE" description="Select list" id="select_list">
        <one-of id="000000049" optional="true">
            <token id="000000050" type-id="KW_ALL" />
            <sequence id="000000051">
                <token id="000000052" type-id="KW_DISTINCT" />
                <sequence id="000000053" optional="true">
                    <token id="000000054" type-id="KW_ON" />
                    <wrapper id="000000055" template="PARENTHESES">
                        <iteration id="000000056" separator="CHR_COMMA">
                            <element ref-id="expression" />
                        </iteration>
                    </wrapper>
                </sequence>
            </sequence>
        </one-of>
        <one-of id="000000057">
            <token id="000000058" type-id="CHR_STAR" />
            <iteration id="000000059" separator="CHR_COMMA">
                <sequence id="000000060">
                    <element ref-id="condition" />
                    <sequence id="000000061" optional="true">
                        <token id="000000062" optional="true" type-id="KW_AS" />
                        <alias-def id="000000063" type="COLUMN" />
                    </sequence>
                </sequence>
            </iteration>
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="FROM clause" id="from_clause">
        <token id="000000064" type-id="KW_FROM" />
        <iteration id="000000065" separator="CHR_COMMA" wrapping-template="PARENTHESES">
            <sequence id="000000066" wrapping-template="PARENTHESES">
                <element ref-id="from_item" />
                <element optional="true" ref-id="join_clause" />
            </sequence>
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="WHERE clause" id="where_clause">
        <token id="000000067" type-id="KW_WHERE" />
        <element ref-id="condition" />
    </element-def>
    <element-def attributes="CLAUSE" description="GROUP BY clause" id="group_by_clause">
        <token id="000000068" type-id="KW_GROUP" />
        <token id="000000069" type-id="KW_BY" />
        <iteration id="000000070" separator="CHR_COMMA">
            <element ref-id="expression" />
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="HAVING clause" id="having_clause">
        <token id="000000071" type-id="KW_HAVING" />
        <iteration id="000000072" separator="CHR_COMMA">
            <element ref-id="condition" />
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="WINDOW clause" id="window_clause">
        <token id="000000073" type-id="KW_WINDOW" />
        <object-def id="000000074" original-name="window_name" type="WINDOW" />
        <token id="000000075" type-id="KW_AS" />
        <wrapper id="000000076" template="PARENTHESES">
            <element ref-id="window_definition" />
        </wrapper>
    </element-def>
    <element-def description="Window definition" id="window_definition">
        <one-of id="000000077">
            <sequence id="000000078">
                <token id="000000079" type-id="KW_PARTITION" />
                <token id="000000080" type-id="KW_BY" />
                <iteration id="000000081" separator="CHR_COMMA">
                    <element ref-id="expression" />
                </iteration>
            </sequence>

            <sequence id="000000082">
                <one-of id="000000083" optional="true">
                    <token id="000000084" type-id="KW_RANGE" />
                    <token id="000000085" type-id="KW_ROWS" />
                </one-of>
                <one-of id="000000086">
                    <element original-name="frame_start" ref-id="frame_start_end" />
                    <sequence id="000000087">
                        <token id="000000088" type-id="KW_BETWEEN" />
                        <element original-name="frame_start" ref-id="frame_start_end" />
                        <token id="000000089" type-id="KW_AND" />
                        <element original-name="frame_end" ref-id="frame_start_end" />
                    </sequence>
                </one-of>
            </sequence>
            <element ref-id="order_by_clause" />
            <object-ref id="000000090" original-name="existing_window_name" type="WINDOW" />
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="ORDER BY clause" id="order_by_clause">
        <sequence id="000000091">
            <token id="000000092" type-id="KW_ORDER" />
            <token id="000000093" type-id="KW_BY" />
            <iteration id="000000094" separator="CHR_COMMA">
                <sequence id="000000095">
                    <element ref-id="expression" />
                    <one-of id="000000096" optional="true">
                        <token id="000000097" type-id="KW_ASC" />
                        <token id="000000098" type-id="KW_DESC" />
                        <sequence id="000000099">
                            <token id="000000100" type-id="KW_USING" />
                            <one-of id="000000101">
                                <token id="000000102" type-id="CHR_LESS" />
                                <token id="000000103" type-id="CHR_GREATER" />
                                <object-ref id="000000104" type="OPERATOR" />
                            </one-of>
                        </sequence>
                    </one-of>
                    <sequence id="000000105" optional="true">
                        <token id="000000106" type-id="KW_NULLS" />
                        <one-of id="000000107">
                            <token id="000000108" type-id="KW_FIRST" />
                            <token id="000000109" type-id="KW_LAST" />
                        </one-of>
                    </sequence>
                </sequence>
            </iteration>
        </sequence>
    </element-def>
    <element-def description="frame_start_end" id="frame_start_end">
        <one-of id="000000110">
            <sequence id="000000111">
                <token id="000000112" type-id="KW_UNBOUNDED" />
                <one-of id="000000113">
                    <token id="000000114" type-id="KW_PRECEDING" />
                    <token id="000000115" type-id="KW_FOLLOWING" />
                </one-of>
            </sequence>
            <sequence id="000000116">
                <token id="000000117" type-id="KW_CURRENT" />
                <token id="000000118" type-id="KW_ROW" />
            </sequence>
            <sequence id="000000119">
                <element ref-id="expression" />
                <one-of exit="true" id="000000120">
                    <token id="000000121" type-id="KW_PRECEDING" />
                    <token id="000000122" type-id="KW_FOLLOWING" />
                </one-of>
            </sequence>
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="LIMIT clause" id="limit_clause">
        <token id="000000123" type-id="KW_LIMIT" />
        <one-of id="000000124">
            <token id="000000125" original-name="count" type-id="INTEGER" />
            <token id="000000126" type-id="KW_ALL" />
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="OFFSET clause" id="offset_clause">
        <token id="000000127" type-id="KW_OFFSET" />
        <token id="000000128" original-name="start" type-id="INTEGER" />
        <one-of id="000000129" optional="true">
            <token id="000000130" type-id="KW_ROW" />
            <token id="000000131" type-id="KW_ROWS" />
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="FETCH clause" id="fetch_clause">
        <token id="000000132" type-id="KW_FETCH" />
        <one-of id="000000133">
            <token id="000000134" type-id="KW_FIRST" />
            <token id="000000135" type-id="KW_NEXT" />
        </one-of>
        <token id="000000136" optional="true" original-name="count" type-id="INTEGER" />
        <one-of id="000000137">
            <token id="000000138" type-id="KW_ROW" />
            <token id="000000139" type-id="KW_ROWS" />
        </one-of>
        <token id="000000140" type-id="KW_ONLY" />
    </element-def>
    <element-def attributes="CLAUSE" description="FOR clause" id="for_clause">
        <iteration id="000000141">
            <sequence id="000000142">
                <token id="000000143" type-id="KW_FOR" />
                <one-of id="000000144">
                    <sequence id="000000145">
                        <token id="000000146" optional="true" type-id="KW_KEY" />
                        <token id="000000147" type-id="KW_SHARE" />
                    </sequence>
                    <sequence id="000000148">
                        <sequence id="000000149" optional="true">
                            <token id="000000150" type-id="KW_NO" />
                            <token id="000000151" type-id="KW_KEY" />
                        </sequence>
                        <token id="000000152" type-id="KW_UPDATE" />
                    </sequence>
                </one-of>
                <sequence id="000000153" optional="true">
                    <token id="000000154" type-id="KW_OF" />
                    <qualified-identifier id="000000155" separator="CHR_DOT">
                        <variant>
                            <object-ref id="000000156" optional="true" referenceable="true" type="SCHEMA" />
                            <object-ref id="000000157" referenceable="true" type="DATASET" />
                        </variant>
                    </qualified-identifier>
                </sequence>
                <token id="000000158" optional="true" type-id="KW_NOWAIT" />
            </sequence>
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="UNION INTERSECT EXCEPT clause" id="union_intersect_except_clause">
        <iteration id="000000159">
            <sequence id="000000160">
                <one-of id="000000161">
                    <token id="000000162" type-id="KW_UNION" />
                    <token id="000000163" type-id="KW_INTERSECT" />
                    <token id="000000164" type-id="KW_EXCEPT" />
                </one-of>
                <one-of id="000000165" optional="true">
                    <token id="000000166" type-id="KW_ALL" />
                    <token id="000000167" type-id="KW_DISTINCT" />
                </one-of>
                <element ref-id="subquery" />
            </sequence>
        </iteration>
    </element-def>

    <element-def description="JOIN type" id="join_type">
        <one-of id="000000168" optional="true">
            <token id="000000169" type-id="KW_INNER" />
            <token id="000000170" type-id="KW_CROSS" />
            <sequence id="000000171">
                <token flavor="keyword" id="000000172" type-id="FN_LEFT" />
                <token id="000000173" optional="true" type-id="KW_OUTER" />
            </sequence>
            <sequence id="000000174">
                <token flavor="keyword" id="000000175" type-id="FN_RIGHT" />
                <token id="000000176" optional="true" type-id="KW_OUTER" />
            </sequence>
            <sequence id="000000177">
                <token id="000000178" type-id="KW_FULL" />
                <token id="000000179" optional="true" type-id="KW_OUTER" />
            </sequence>
        </one-of>
        <token id="000000180" type-id="KW_JOIN" />
    </element-def>
    <element-def description="From item" id="from_item" wrapping-template="PARENTHESES">
        <one-of id="000000181">
            <sequence id="000000182">
                <token id="000000183" optional="true" type-id="KW_ONLY" />
                <qualified-identifier id="000000184" separator="CHR_DOT">
                    <variant>
                        <alias-ref id="000000185" type="DATASET" />
                    </variant>
                    <variant>
                        <object-ref id="000000186" optional="true" referenceable="true" type="SCHEMA" />
                        <object-ref id="000000187" referenceable="true" type="DATASET" />
                    </variant>
                    <variant>
                        <object-ref id="000000188" optional="true" referenceable="true" type="SCHEMA" />
                        <object-ref id="000000189" type="FUNCTION" />
                    </variant>
                </qualified-identifier>
                <one-of id="000000190" optional="true">
                    <token id="000000191" optional="true" type-id="CHR_STAR" />
                    <wrapper id="000000192" template="PARENTHESES">
                        <element ref-id="expression_list" />
                    </wrapper>
                </one-of>
            </sequence>
            <sequence id="000000193">
                <token id="000000194" optional="true" type-id="KW_LATERAL" />
                <one-of id="000000195">
                    <element ref-id="subquery" />
                    <element ref-id="function_expression" />
                </one-of>
            </sequence>
        </one-of>
        <element optional="true" ref-id="from_item_alias" />
        <iteration id="000000196" optional="true">
            <element optional="true" ref-id="join_clause" />
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="JOIN clause" id="from_item_alias">
        <token id="000000197" optional="true" type-id="KW_AS" />
        <alias-def id="000000198" type="DATASET" />
        <wrapper id="000000199" optional="true" template="PARENTHESES">
            <iteration id="000000200" separator="CHR_COMMA">
                <qualified-identifier id="000000201" separator="CHR_DOT">
                    <variant>
                        <alias-ref id="000000202" type="COLUMN" />
                    </variant>
                    <variant>
                        <alias-def id="000000203" type="COLUMN" />
                    </variant>
                    <variant>
                        <alias-ref id="000000204" optional="true" type="DATASET" />
                        <object-ref id="000000205" type="COLUMN" />
                    </variant>
                    <variant>
                        <object-ref id="000000206" local="true" optional="true" type="SCHEMA" />
                        <object-ref id="000000207" local="true" optional="true" type="DATASET" />
                        <object-ref id="000000208" type="COLUMN" />
                    </variant>
                </qualified-identifier>
            </iteration>
        </wrapper>
    </element-def>
    <element-def attributes="CLAUSE" description="JOIN clause" id="join_clause">
        <iteration id="000000209" optional="true">
            <sequence id="000000210">
                <token id="000000211" optional="true" type-id="KW_NATURAL" />
                <element ref-id="join_type" />
                <element ref-id="from_item" />
                <one-of id="000000212" optional="true">
                    <sequence id="000000213">
                        <token id="000000214" type-id="KW_ON" />
                        <element original-name="join_condition" ref-id="condition" />
                    </sequence>
                    <sequence id="000000215">
                        <token id="000000216" type-id="KW_USING" />
                        <wrapper id="000000217" template="PARENTHESES">
                            <qualified-identifier id="000000218" separator="CHR_DOT">
                                <variant>
                                    <alias-ref id="000000219" optional="true" type="DATASET" />
                                    <object-ref id="000000220" type="COLUMN" />
                                </variant>
                                <variant>
                                    <object-ref id="000000221" local="true" optional="true" type="SCHEMA" />
                                    <object-ref id="000000222" local="true" optional="true" type="DATASET" />
                                    <object-ref id="000000223" type="COLUMN" />
                                </variant>
                            </qualified-identifier>
                        </wrapper>
                    </sequence>
                </one-of>
            </sequence>
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="WITH clause" id="with_clause">
        <token id="000000224" type-id="KW_WITH" />
        <token id="000000225" optional="true" type-id="KW_RECURSIVE" />
        <iteration id="000000226" separator="CHR_COMMA">
            <sequence id="000000227">
                <alias-def id="000000228" type="DATASET" />
                <wrapper id="000000229" optional="true" template="PARENTHESES">
                    <iteration id="000000230" separator="CHR_COMMA">
                        <alias-def id="000000231" original-name="column_name" type="COLUMN" />
                    </iteration>
                </wrapper>
                <token id="000000232" type-id="KW_AS" />
                <wrapper id="000000233" template="PARENTHESES">
                    <one-of id="000000234">
                        <element ref-id="subquery" />
                        <element ref-id="values_clause" />
                        <element ref-id="insert_statement" />
                        <element ref-id="update_statement" />
                        <element ref-id="delete_statement" />
                    </one-of>
                </wrapper>
            </sequence>
        </iteration>
    </element-def>

    <!-- ========= EXPRESSION ========= -->
    <element-def description="Expression" id="expression">
        <iteration id="000000235" separator="OPR_CONCATENATION, CHR_STAR, CHR_SLASH, CHR_PLUS, CHR_MINUS, CHR_AND">
            <sequence id="000000236">
                <sequence id="000000237" wrapping-template="PARENTHESES">
                    <one-of id="000000238" optional="true">
                        <token id="000000239" type-id="CHR_PLUS" />
                        <token id="000000240" type-id="CHR_MINUS" />
                        <token id="000000241" type-id="CHR_AND" />
                        <token id="000000242" type-id="KW_PRIOR" />
                        <token id="000000243" type-id="KW_NOT" />
                    </one-of>
                    <one-of id="000000244">
                        <element ref-id="expression" />
                        <element ref-id="expr" />
                    </one-of>
                </sequence>

                <iteration id="000000245" optional="true">
                    <one-of id="000000246">
                        <sequence id="000000247">
                            <token id="000000248" type-id="CHR_LEFT_BRACKET" />
                            <element optional="true" ref-id="expression_list" />
                            <token id="000000249" type-id="CHR_RIGHT_BRACKET" />
                        </sequence>
                        <sequence id="000000250">
                            <token id="000000251" type-id="CHR_DOT" />
                            <object-ref id="000000252" type="ANY" />
                        </sequence>
                        <iteration id="000000253" optional="true">
                            <element optional="true" ref-id="postfix_cast_expression" />
                        </iteration>
                    </one-of>
                </iteration>
            </sequence>
        </iteration>
    </element-def>
    <element-def description="Expr" id="expr" wrapping-template="PARENTHESES">
        <one-of id="000000254">
            <element ref-id="subquery" />
            <element ref-id="array_expression" />
            <element ref-id="case_expression" />
            <element ref-id="cast_expression" />
            <element ref-id="sequence_expression" />
            <element ref-id="simple_expression" />
            <exec-variable id="000000255" />
        </one-of>
        <iteration id="000000256" optional="true">
            <element optional="true" ref-id="postfix_cast_expression" />
        </iteration>
    </element-def>
    <element-def description="Postfix cast expression" id="postfix_cast_expression">
        <token id="000000257" type-id="OPR_CAST_PREFIX" />
        <one-of id="000000258">
            <element ref-id="data_type" />
            <qualified-identifier id="000000259" separator="CHR_DOT">
                <variant>
                    <object-ref id="000000260" optional="true" type="SCHEMA" />
                    <object-ref id="000000261" local="true" type="TYPE" />
                </variant>
            </qualified-identifier>
        </one-of>
    </element-def>
    <element-def description="Array expression" id="array_expression">
        <one-of id="000000262">
            <token flavor="function" id="000000263" type-id="KW_ANY" />
            <token flavor="function" id="000000264" type-id="KW_SOME" />
            <token flavor="function" id="000000265" type-id="KW_ALL" />
        </one-of>
        <wrapper id="000000266" template="PARENTHESES">
            <one-of id="000000267">
                <sequence id="000000268">
                    <token flavor="keyword" id="000000269" type-id="DT_ARRAY" />
                    <wrapper begin-token="CHR_LEFT_BRACKET" end-token="CHR_RIGHT_BRACKET" id="000000270">
                        <iteration id="000000271" separator="CHR_COMMA">
                            <element ref-id="expression" />
                        </iteration>
                    </wrapper>
                </sequence>
                <element ref-id="simple_expression" />
            </one-of>
        </wrapper>
    </element-def>
    <element-def description="Simple expression" id="simple_expression">
        <one-of id="000000272" sortable="true">
            <sequence id="000000273">
                <qualified-identifier id="000000274" separator="CHR_DOT">
                    <variant>
                        <alias-ref id="000000275" optional="true" type="DATASET" />
                        <token id="000000276" type-id="DT_OID" />
                    </variant>
                    <variant>
                        <alias-ref id="000000277" optional="true" type="DATASET" />
                        <object-ref id="000000278" type="COLUMN" />
                    </variant>
                    <variant>
                        <object-ref id="000000279" local="true" optional="true" type="SCHEMA" />
                        <object-ref id="000000280" local="true" optional="true" type="DATASET" />
                        <object-ref id="000000281" type="COLUMN" />
                    </variant>
                    <variant>
                        <object-ref id="000000282" local="true" optional="true" type="SCHEMA" />
                        <object-ref id="000000283" local="true" optional="true" type="DATASET" />
                        <token id="000000284" type-id="CHR_STAR" />
                    </variant>
                    <variant>
                        <object-ref id="000000285" local="true" optional="true" type="SCHEMA" />
                        <object-ref id="000000286" local="true" optional="true" type="DATASET" />
                        <token id="000000287" type-id="DT_OID" />
                    </variant>
                    <variant>
                        <alias-ref id="000000288" optional="true" type="DATASET" />
                        <alias-ref id="000000289" type="COLUMN" />
                    </variant>
                    <variant>
                        <alias-ref id="000000290" type="DATASET" />
                        <token id="000000291" type-id="CHR_STAR" />
                    </variant>
                    <variant>
                        <alias-ref id="000000292" type="DATASET" />
                        <token id="000000293" type-id="DT_OID" />
                    </variant>
                    <variant>
                        <object-ref id="000000294" optional="true" type="SCHEMA" />
                        <object-ref id="000000295" type="FUNCTION" />
                    </variant>
                </qualified-identifier>
                <wrapper id="000000296" optional="true" template="PARENTHESES">
                    <iteration id="000000297" optional="true" separator="CHR_COMMA">
                        <element ref-id="expression" />
                    </iteration>
                </wrapper>
            </sequence>
            <element ref-id="number" />
            <token id="000000298" original-name="text" type-id="STRING" />
            <token id="000000299" type-id="KW_NULL" />
            <token id="000000300" type-id="KW_TRUE" />
            <token id="000000301" type-id="KW_FALSE" />
        </one-of>
        <iteration id="000000302" optional="true">
            <sequence id="000000303">
                <token id="000000304" type-id="CHR_LEFT_BRACKET" />
                <element optional="true" ref-id="expression_list" />
                <token id="000000305" type-id="CHR_RIGHT_BRACKET" />
            </sequence>
        </iteration>
        <token id="000000306" optional="true" type-id="CT_OUTER_JOIN" />
    </element-def>
    <element-def description="CASE expression" id="case_expression">
        <token id="000000307" type-id="KW_CASE" />
        <element optional="true" ref-id="expression" />
        <iteration id="000000308">
            <sequence id="000000309">
                <token id="000000310" type-id="KW_WHEN" />
                <element ref-id="condition" />
                <token id="000000311" type-id="KW_THEN" />
                <element ref-id="condition" />
            </sequence>
        </iteration>
        <sequence id="000000312" optional="true">
            <token id="000000313" type-id="KW_ELSE" />
            <element ref-id="condition" />
        </sequence>
        <token id="000000314" type-id="KW_END" />
    </element-def>
    <element-def description="Cast expression" id="cast_expression">
        <one-of id="000000315">
            <sequence id="000000316">
                <token id="000000317" type-id="KW_CAST" />
                <wrapper id="000000318" template="PARENTHESES">
                    <sequence id="000000319">
                        <element ref-id="expression" />
                        <token id="000000320" type-id="KW_AS" />
                        <element ref-id="data_type" />
                    </sequence>
                </wrapper>
            </sequence>
            <sequence id="000000321">
                <element ref-id="data_type" />
                <wrapper id="000000322" template="PARENTHESES">
                    <element ref-id="expression" />
                </wrapper>
            </sequence>
        </one-of>
    </element-def>
    <element-def description="Sequence expression" id="sequence_expression">
        <one-of id="000000323">
            <sequence id="000000324">
                <token id="000000325" type-id="FN_CURRVAL" />
                <wrapper id="000000326" template="PARENTHESES">
                    <token id="000000327" type-id="STRING" />
                </wrapper>
            </sequence>
            <sequence id="000000328">
                <token id="000000329" type-id="FN_NEXTVAL" />
                <wrapper id="000000330" template="PARENTHESES">
                    <token id="000000331" type-id="STRING" />
                </wrapper>
            </sequence>
            <sequence id="000000332">
                <token id="000000333" type-id="FN_LASTVAL" />
                <token id="000000334" type-id="CHR_LEFT_PARENTHESIS" />
                <token id="000000335" type-id="CHR_RIGHT_PARENTHESIS" />
            </sequence>
            <sequence id="000000336">
                <token id="000000337" type-id="FN_SETVAL" />
                <wrapper id="000000338" template="PARENTHESES">
                    <sequence id="000000339">
                        <token id="000000340" type-id="STRING" />
                        <token id="000000341" type-id="INTEGER" />
                        <one-of id="000000342" optional="true">
                            <token id="000000343" type-id="KW_TRUE" />
                            <token id="000000344" type-id="KW_FALSE" />
                        </one-of>
                    </sequence>
                </wrapper>
            </sequence>
        </one-of>
    </element-def>

    <element-def description="Function expression" id="function_expression">
        <one-of id="000000345">
            <sequence id="000000346">
                <qualified-identifier id="000000347" separator="CHR_DOT">
                    <variant>
                        <object-ref id="000000348" optional="true" type="SCHEMA" />
                        <object-ref id="000000349" type="FUNCTION" />
                    </variant>
                </qualified-identifier>
                <wrapper exit="true" id="000000350" template="PARENTHESES">
                    <iteration id="000000351" optional="true" separator="CHR_COMMA">
                        <element ref-id="expression" />
                    </iteration>
                </wrapper>
            </sequence>
        </one-of>
    </element-def>

    <element-def description="Expression list" id="expression_list">
        <iteration id="000000352" separator="CHR_COMMA" wrapping-template="PARENTHESES">
            <element ref-id="expression" />
        </iteration>
    </element-def>


    <!-- ========= CONDITIONS ========= -->
    <element-def description="Condition" id="condition" wrapping-template="PARENTHESES">
        <one-of id="000000353">
            <iteration formatting-indent="NORMAL" id="000000354" separator="KW_AND, KW_OR">
                <element ref-id="condition" />
            </iteration>
            <sequence id="000000355" wrapping-template="PARENTHESES">
                <element ref-id="expression_list" />
                <one-of id="000000356" optional="true">
                    <sequence id="000000357">
                        <token id="000000358" optional="true" type-id="KW_NOT" />
                        <one-of id="000000359">
                            <sequence id="000000360">
                                <token exit="true" id="000000361" type-id="KW_IN" />
                                <wrapper id="000000362" template="PARENTHESES">
                                    <one-of id="000000363">
                                        <element ref-id="subquery" />
                                        <element ref-id="expression_list" />
                                    </one-of>
                                </wrapper>
                            </sequence>
                            <sequence id="000000364">
                                <token id="000000365" type-id="KW_LIKE" />
                                <element ref-id="expression" />
                            </sequence>
                            <sequence id="000000366">
                                <token id="000000367" type-id="KW_EXISTS" />
                                <wrapper id="000000368" template="PARENTHESES">
                                    <element ref-id="subquery" />
                                </wrapper>
                            </sequence>
                            <sequence id="000000369">
                                <sequence id="000000370">
                                    <token id="000000371" type-id="KW_BETWEEN" />
                                    <element ref-id="expression" />
                                    <token id="000000372" type-id="KW_AND" />
                                    <element ref-id="expression" />
                                </sequence>
                            </sequence>
                        </one-of>
                    </sequence>
                    <sequence id="000000373">
                        <one-of id="000000374">
                            <element ref-id="equality_operator" />
                            <element ref-id="greater_less_operator" />
                        </one-of>
                        <one-of id="000000375">
                            <wrapper id="000000376" template="PARENTHESES">
                                <element ref-id="subquery" />
                            </wrapper>
                            <element ref-id="expression_list" />
                        </one-of>
                    </sequence>
                    <sequence id="000000377">
                        <token id="000000378" type-id="KW_IS" />
                        <token id="000000379" optional="true" type-id="KW_NOT" />
                        <token id="000000380" type-id="KW_NULL" />
                    </sequence>
                </one-of>
            </sequence>
        </one-of>
    </element-def>



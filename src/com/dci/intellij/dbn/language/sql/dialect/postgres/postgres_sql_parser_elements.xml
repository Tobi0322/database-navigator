<?xml version="1.0" encoding="UTF-8"?>
<!--<!DOCTYPE element-defs SYSTEM "../../../common/definition/language-parser-elements.dtd">--><element-defs language="POSTGRES-SQL" version="9.3">
    <element-def attributes="ROOT, STRUCTURE" description="SQL block" icon="FILE_BLOCK_SQL" id="sql_block">
        <iteration id="000000000">
            <one-of attributes="STATEMENT, SCOPE_ISOLATION" formatting-indent="ABSOLUTE_NONE" id="000000001">
                <element ref-id="select_statement" />
            </one-of>
        </iteration>
    </element-def>


    <!-- ========= select ========= -->
    <element-def attributes="EXECUTABLE, DATA_MANIPULATION, QUERY, STRUCTURE" description="SELECT statement" id="select_statement">
        <element ref-id="subquery" />
        <element ref-id="end" />
    </element-def>
    <element-def attributes="EXECUTABLE, DATA_MANIPULATION, QUERY" description="Subquery" id="subquery" virtual-object="DATASET">
        <one-of id="000000002">
            <sequence id="000000003">
                <element optional="true" ref-id="with_clause" />
                <token id="000000004" type-id="KW_SELECT" />
                <element ref-id="select_list" />
                <element optional="true" ref-id="from_clause" />
                <element optional="true" ref-id="where_clause" />
                <element optional="true" ref-id="group_by_clause" />
                <element optional="true" ref-id="having_clause" />
                <element optional="true" ref-id="window_clause" />
                <iteration id="000000005" optional="true">
                    <element ref-id="union_intersect_except_clause" />
                </iteration>
                <element optional="true" ref-id="order_by_clause" />
                <element optional="true" ref-id="limit_clause" />
                <element optional="true" ref-id="offset_clause" />
                <element optional="true" ref-id="fetch_clause" />
                <element optional="true" ref-id="select_for_clause" />
            </sequence>
            <wrapper id="000000006" template="PARENTHESES">
                <element ref-id="subquery" />
            </wrapper>
        </one-of>
        <element optional="true" ref-id="order_by_clause" />
    </element-def>
    <element-def attributes="CLAUSE" description="Select list" id="select_list">
        <one-of id="000000007" optional="true">
            <token id="000000008" type-id="KW_ALL" />
            <sequence id="000000009">
                <token id="000000010" type-id="KW_DISTINCT" />
                <sequence id="000000011" optional="true">
                    <token id="000000012" type-id="KW_ON" />
                    <wrapper id="000000013" template="PARENTHESES">
                        <iteration id="000000014" separator="CHR_COMMA">
                            <element ref-id="expression" />
                        </iteration>
                    </wrapper>
                </sequence>
            </sequence>
        </one-of>
        <one-of id="000000015">
            <token id="000000016" type-id="CHR_STAR" />
            <iteration id="000000017" separator="CHR_COMMA">
                <sequence id="000000018">
                    <element ref-id="expression" />
                    <sequence id="000000019" optional="true">
                        <token id="000000020" optional="true" type-id="KW_AS" />
                        <alias-def id="000000021" type="COLUMN" />
                    </sequence>
                </sequence>
            </iteration>
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="FROM clause" id="from_clause">
        <token id="000000022" type-id="KW_FROM" />
        <iteration id="000000023" separator="CHR_COMMA">
            <sequence id="000000024">
                <element ref-id="subquery_from_item" />
            </sequence>
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="WHERE clause" id="where_clause">
        <token id="000000025" type-id="KW_WHERE" />
        <element ref-id="condition" />
    </element-def>
    <element-def attributes="CLAUSE" description="GROUP BY clause" id="group_by_clause">
        <token id="000000026" type-id="KW_GROUP" />
        <token id="000000027" type-id="KW_BY" />
        <iteration id="000000028" separator="CHR_COMMA">
            <element ref-id="expression" />
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="HAVING clause" id="having_clause">
        <token id="000000029" type-id="KW_HAVING" />
        <iteration id="000000030" separator="CHR_COMMA">
            <element ref-id="condition" />
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="WINDOW clause" id="window_clause">
        <token id="000000031" type-id="KW_WINDOW" />
        <object-def id="000000032" original-name="window_name" type="WINDOW" />
        <token id="000000033" type-id="KW_AS" />
        <wrapper id="000000034" template="PARENTHESES">
            <element ref-id="window_definition" />
        </wrapper>
    </element-def>
    <element-def description="Window definition" id="window_definition">
        <one-of id="000000035">
            <sequence id="000000036">
                <token id="000000037" type-id="KW_PARTITION" />
                <token id="000000038" type-id="KW_BY" />
                <iteration id="000000039" separator="CHR_COMMA">
                    <element ref-id="expression" />
                </iteration>
            </sequence>

            <sequence id="000000040">
                <one-of id="000000041" optional="true">
                    <token id="000000042" type-id="KW_RANGE" />
                    <token id="000000043" type-id="KW_ROWS" />
                </one-of>
                <one-of id="000000044">
                    <element original-name="frame_start" ref-id="frame_start_end" />
                    <sequence id="000000045">
                        <token id="000000046" type-id="KW_BETWEEN" />
                        <element original-name="frame_start" ref-id="frame_start_end" />
                        <token id="000000047" type-id="KW_AND" />
                        <element original-name="frame_end" ref-id="frame_start_end" />
                    </sequence>
                </one-of>
            </sequence>
            <element ref-id="order_by_clause" />
            <object-ref id="000000048" original-name="existing_window_name" type="WINDOW" />
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="ORDER BY clause" id="order_by_clause">
        <sequence id="000000049">
            <token id="000000050" type-id="KW_ORDER" />
            <token id="000000051" type-id="KW_BY" />
            <iteration id="000000052" separator="CHR_COMMA">
                <sequence id="000000053">
                    <element ref-id="expression" />
                    <one-of id="000000054" optional="true">
                        <token id="000000055" type-id="KW_ASC" />
                        <token id="000000056" type-id="KW_DESC" />
                        <sequence id="000000057">
                            <token id="000000058" type-id="KW_USING" />
                            <one-of id="000000059">
                                <token id="000000060" type-id="CHR_LESS" />
                                <token id="000000061" type-id="CHR_GREATER" />
                                <object-ref id="000000062" type="OPERATOR" />
                            </one-of>
                        </sequence>
                    </one-of>
                    <sequence id="000000063" optional="true">
                        <token id="000000064" type-id="KW_NULLS" />
                        <one-of id="000000065">
                            <token id="000000066" type-id="KW_FIRST" />
                            <token id="000000067" type-id="KW_LAST" />
                        </one-of>
                    </sequence>
                </sequence>
            </iteration>
        </sequence>
    </element-def>
    <element-def description="frame_start_end" id="frame_start_end">
        <one-of id="000000068">
            <sequence id="000000069">
                <token id="000000070" type-id="KW_UNBOUNDED" />
                <one-of id="000000071">
                    <token id="000000072" type-id="KW_PRECEDING" />
                    <token id="000000073" type-id="KW_FOLLOWING" />
                </one-of>
            </sequence>
            <sequence id="000000074">
                <token id="000000075" type-id="KW_CURRENT" />
                <token id="000000076" type-id="KW_ROW" />
            </sequence>
            <sequence id="000000077">
                <element ref-id="expression" />
                <one-of exit="true" id="000000078">
                    <token id="000000079" type-id="KW_PRECEDING" />
                    <token id="000000080" type-id="KW_FOLLOWING" />
                </one-of>
            </sequence>
        </one-of>
    </element-def>
    <element-def attributes="CLAUSE" description="UNION INTERSECT EXCEPT clause" id="union_intersect_except_clause">
        <one-of id="000000081">
            <token id="000000082" type-id="KW_UNION" />
            <token id="000000083" type-id="KW_INTERSECT" />
            <token id="000000084" type-id="KW_EXCEPT" />
        </one-of>
        <one-of id="000000085" optional="true">
            <token id="000000086" type-id="KW_ALL" />
            <token id="000000087" type-id="KW_DISTINCT" />
        </one-of>
        <element ref-id="subquery" />
    </element-def>

    <element-def description="JOIN type" id="join_type">
        <one-of id="000000088" optional="true">
            <token id="000000089" type-id="KW_INNER" />
            <token id="000000090" type-id="KW_CROSS" />
            <sequence id="000000091">
                <token flavor="keyword" id="000000092" type-id="FN_LEFT" />
                <token id="000000093" optional="true" type-id="KW_OUTER" />
            </sequence>
            <sequence id="000000094">
                <token flavor="keyword" id="000000095" type-id="FN_RIGHT" />
                <token id="000000096" optional="true" type-id="KW_OUTER" />
            </sequence>
            <sequence id="000000097">
                <token id="000000098" type-id="KW_FULL" />
                <token id="000000099" optional="true" type-id="KW_OUTER" />
            </sequence>
        </one-of>
        <token id="000000100" type-id="KW_JOIN" />
    </element-def>
    <element-def description="From item" id="subquery_from_item">
        <one-of id="000000101">
            <sequence id="000000102">
                <token id="000000103" optional="true" type-id="KW_ONLY" />
                <qualified-identifier id="000000104" separator="CHR_DOT">
                    <variant>
                        <alias-ref id="000000105" type="DATASET" />
                    </variant>
                    <variant>
                        <object-ref id="000000106" local="true" optional="true" type="SCHEMA" />
                        <object-ref id="000000107" type="DATASET" />
                    </variant>
                </qualified-identifier>
                <token id="000000108" optional="true" type-id="CHR_STAR" />
            </sequence>
            <sequence id="000000109">
                <token id="000000110" optional="true" type-id="KW_LATERAL" />
                <one-of id="000000111">
                    <wrapper id="000000112" template="PARENTHESES">
                        <element ref-id="subquery" />
                    </wrapper>
                    <element ref-id="function_expression" />
                </one-of>
            </sequence>
        </one-of>
        <sequence id="000000113" optional="true">
            <token id="000000114" optional="true" type-id="KW_AS" />
            <alias-def id="000000115" type="DATASET" />
            <wrapper id="000000116" optional="true" template="PARENTHESES">
                <iteration id="000000117" separator="CHR_COMMA">
                    <qualified-identifier id="000000118" separator="CHR_DOT">
                        <variant>
                            <alias-ref id="000000119" type="COLUMN" />
                        </variant>
                        <variant>
                            <alias-def id="000000120" type="COLUMN" />
                        </variant>
                        <variant>
                            <alias-ref id="000000121" optional="true" type="DATASET" />
                            <object-ref id="000000122" type="COLUMN" />
                        </variant>
                        <variant>
                            <object-ref id="000000123" local="true" optional="true" type="SCHEMA" />
                            <object-ref id="000000124" local="true" optional="true" type="DATASET" />
                            <object-ref id="000000125" type="COLUMN" />
                        </variant>
                    </qualified-identifier>
                </iteration>
            </wrapper>
        </sequence>
        <iteration id="000000126" optional="true">
            <sequence id="000000127">
                <token id="000000128" optional="true" type-id="KW_NATURAL" />
                <element ref-id="join_type" />
                <element ref-id="subquery_from_item" />
                <one-of id="000000129" optional="true">
                    <sequence id="000000130">
                        <token id="000000131" type-id="KW_ON" />
                        <element original-name="join_condition" ref-id="condition" />
                    </sequence>
                    <sequence id="000000132">
                        <token id="000000133" type-id="KW_USING" />
                        <wrapper id="000000134" template="PARENTHESES">
                            <qualified-identifier id="000000135" separator="CHR_DOT">
                                <variant>
                                    <alias-ref id="000000136" optional="true" type="DATASET" />
                                    <object-ref id="000000137" type="COLUMN" />
                                </variant>
                                <variant>
                                    <object-ref id="000000138" local="true" optional="true" type="SCHEMA" />
                                    <object-ref id="000000139" local="true" optional="true" type="DATASET" />
                                    <object-ref id="000000140" type="COLUMN" />
                                </variant>
                            </qualified-identifier>
                        </wrapper>
                    </sequence>
                </one-of>
            </sequence>
        </iteration>
    </element-def>
    <element-def attributes="CLAUSE" description="WITH clause" id="with_clause">
        <token id="000000141" type-id="KW_WITH" />
        <token id="000000142" optional="true" type-id="KW_RECURSIVE" />
        <iteration id="000000143" separator="CHR_COMMA">
            <sequence id="000000144">
                <alias-def id="000000145" type="DATASET" />
                <wrapper id="000000146" optional="true" template="PARENTHESES">
                    <iteration id="000000147" separator="CHR_COMMA">
                        <alias-def id="000000148" original-name="column_name" type="COLUMN" />
                    </iteration>
                </wrapper>
                <token id="000000149" type-id="KW_AS" />
                <wrapper id="000000150" template="PARENTHESES">
                    <one-of id="000000151">
                        <element ref-id="subquery" />
                        <element ref-id="with_query_values_clause" />
                        <element ref-id="with_query_insert_clause" />
                        <element ref-id="with_query_update_clause" />
                        <element ref-id="with_query_delete_clause" />
                    </one-of>
                </wrapper>
            </sequence>
        </iteration>
    </element-def>

    <!-- ========= EXPRESSION ========= -->
    <element-def description="Expression" id="expression">
        <iteration id="000000152" separator="OPR_CONCATENATION, CHR_STAR, CHR_SLASH, CHR_PLUS, CHR_MINUS">
            <wrapper id="000000153" template="PARENTHESES" wrapping-optional="true">
                <sequence id="000000154">
                    <one-of id="000000155" optional="true">
                        <token id="000000156" type-id="CHR_PLUS" />
                        <token id="000000157" type-id="CHR_MINUS" />
                        <token id="000000158" type-id="KW_PRIOR" />
                        <token id="000000159" type-id="KW_NOT" />
                    </one-of>
                    <one-of id="000000160">
                        <element ref-id="expression" />
                        <sequence id="000000161">
                            <wrapper template="PARENTHESES" wrapping-optional="true" id="000000162">
                                <element ref-id="expr" />
                            </wrapper>
                            <element optional="true" ref-id="postfix_cast_expression" />
                        </sequence>
                        <element ref-id="subquery" />
                    </one-of>
                </sequence>
            </wrapper>
        </iteration>
    </element-def>
    <element-def description="Expr" id="expr">
        <one-of id="000000163">
            <element ref-id="array_expression" />
            <element ref-id="case_expression" />
            <element ref-id="cast_expression" />
            <element ref-id="sequence_expression" />
            <element ref-id="function_expression" />
            <element ref-id="simple_expression" />
            <exec-variable id="000000164" />
        </one-of>
    </element-def>
    <element-def description="Postfix cast expression" id="postfix_cast_expression">
        <token id="000000165" type-id="OPR_CAST_PREFIX" />
        <one-of id="000000166">
            <element ref-id="data_type" />
            <qualified-identifier id="000000167" separator="CHR_DOT">
                <variant>
                    <object-ref id="000000168" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000169" local="true" type="TYPE" />
                </variant>
            </qualified-identifier>
        </one-of>
    </element-def>
    <element-def description="Array expression" id="array_expression">
        <one-of id="000000170">
            <token flavor="function" id="000000171" type-id="KW_ANY" />
            <token flavor="function" id="000000172" type-id="KW_SOME" />
            <token flavor="function" id="000000173" type-id="KW_ALL" />
        </one-of>
        <wrapper id="000000174" template="PARENTHESES">
            <one-of id="000000175">
                <sequence id="000000176">
                    <token flavor="keyword" id="000000177" type-id="DT_ARRAY" />
                    <wrapper begin-token="CHR_LEFT_BRACKET" end-token="CHR_RIGHT_BRACKET" id="000000178">
                        <iteration id="000000179" separator="CHR_COMMA">
                            <element ref-id="simple_expression" />
                        </iteration>
                    </wrapper>
                </sequence>
                <element ref-id="simple_expression" />
            </one-of>
        </wrapper>
    </element-def>
    <element-def description="Simple expression" id="simple_expression">
        <one-of id="000000180" sortable="true">
            <qualified-identifier id="000000181" separator="CHR_DOT">
                <variant>
                    <alias-ref id="000000182" optional="true" type="DATASET" />
                    <object-ref id="000000183" type="COLUMN" />
                </variant>
                <variant>
                    <object-ref id="000000184" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000185" local="true" optional="true" type="DATASET" />
                    <object-ref id="000000186" type="COLUMN" />
                </variant>
                <variant>
                    <object-ref id="000000187" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000188" local="true" optional="true" type="DATASET" />
                    <token id="000000189" type-id="CHR_STAR" />
                </variant>
                <variant>
                    <object-ref id="000000190" local="true" optional="true" type="SCHEMA" />
                    <object-ref id="000000191" local="true" optional="true" type="DATASET" />
                    <token flavor="identifier" id="000000192" type-id="DT_OID" />
                </variant>
                <variant>
                    <alias-ref id="000000193" optional="true" type="DATASET" />
                    <alias-ref id="000000194" type="COLUMN" />
                </variant>
                <variant>
                    <object-ref id="000000195" optional="true" type="SCHEMA" />
                    <object-ref id="000000196" type="FUNCTION" />
                </variant>
                <variant>
                    <alias-ref id="000000197" type="DATASET" />
                    <token id="000000198" type-id="CHR_STAR" />
                </variant>
                <variant>
                    <alias-ref id="000000199" type="DATASET" />
                    <token flavor="identifier" id="000000200" type-id="DT_OID" />
                </variant>
            </qualified-identifier>
            <token id="000000201" original-name="text" type-id="STRING" />
            <element ref-id="number" />
            <token id="000000202" type-id="KW_NULL" />
        </one-of>
        <sequence id="000000203" optional="true">
            <token id="000000204" type-id="CHR_LEFT_PARENTHESIS" />
            <token id="000000205" type-id="CHR_RIGHT_PARENTHESIS" />
        </sequence>
        <iteration id="000000206" optional="true">
            <sequence id="000000207">
                <token id="000000208" type-id="CHR_LEFT_BRACKET" />
                <token id="000000209" type-id="INTEGER" />
                <token id="000000210" type-id="CHR_RIGHT_BRACKET" />
            </sequence>
        </iteration>
        <token id="000000211" optional="true" type-id="CT_OUTER_JOIN" />
    </element-def>
    <element-def description="Cast expression" id="cast_expression">
        <one-of id="000000212">
            <sequence id="000000213">
                <token id="000000214" type-id="KW_CAST" />
                <wrapper id="000000215" template="PARENTHESES">
                    <sequence id="000000216">
                        <element ref-id="expression" />
                        <token id="000000217" type-id="KW_AS" />
                        <element ref-id="data_type" />
                    </sequence>
                </wrapper>
            </sequence>
            <sequence id="000000218">
                <element ref-id="data_type" />
                <wrapper id="000000219" template="PARENTHESES">
                    <element ref-id="expression" />
                </wrapper>
            </sequence>
        </one-of>
    </element-def>
    <element-def description="Sequence expression" id="sequence_expression">
        <one-of id="000000220">
            <sequence id="000000221">
                <token id="000000222" type-id="FN_CURRVAL" />
                <wrapper id="000000223" template="PARENTHESES">
                    <token id="000000224" type-id="STRING" />
                </wrapper>
            </sequence>
            <sequence id="000000225">
                <token id="000000226" type-id="FN_NEXTVAL" />
                <wrapper id="000000227" template="PARENTHESES">
                    <token id="000000228" type-id="STRING" />
                </wrapper>
            </sequence>
            <sequence id="000000229">
                <token id="000000230" type-id="FN_LASTVAL" />
                <token id="000000231" type-id="CHR_LEFT_PARENTHESIS" />
                <token id="000000232" type-id="CHR_RIGHT_PARENTHESIS" />
            </sequence>
            <sequence id="000000233">
                <token id="000000234" type-id="FN_SETVAL" />
                <wrapper id="000000235" template="PARENTHESES">
                    <sequence id="000000236">
                        <token id="000000237" type-id="STRING" />
                        <token id="000000238" type-id="INTEGER" />
                        <one-of id="000000239" optional="true">
                            <token id="000000240" type-id="KW_TRUE" />
                            <token id="000000241" type-id="KW_FALSE" />
                        </one-of>
                    </sequence>
                </wrapper>
            </sequence>
        </one-of>
    </element-def>

    <element-def description="Function expression" id="function_expression">
        <one-of id="000000242">
            <sequence id="000000243">
                <qualified-identifier id="000000244" separator="CHR_DOT">
                    <variant>
                        <object-ref id="000000245" local="true" optional="true" type="SCHEMA" />
                        <object-ref id="000000246" type="FUNCTION" />
                    </variant>
                </qualified-identifier>
                <wrapper exit="true" id="000000247" template="PARENTHESES">
                    <iteration id="000000248" optional="true" separator="CHR_COMMA">
                        <element ref-id="expression" />
                    </iteration>
                </wrapper>
            </sequence>
        </one-of>
    </element-def>

    <element-def description="Expression list" id="expression_list">
        <one-of id="000000249" sortable="true">
            <iteration id="000000250" separator="CHR_COMMA">
                <element ref-id="expr" />
            </iteration>
            <wrapper id="000000251" template="PARENTHESES">
                <iteration id="000000252" separator="CHR_COMMA">
                    <element ref-id="expr" />
                </iteration>
            </wrapper>
        </one-of>
    </element-def>


    <!-- ========= CONDITIONS ========= -->
    <element-def description="Condition" id="condition">
        <one-of id="000000253">
            <iteration formatting-indent="NORMAL" id="000000254" original-name="compound_condition" separator="KW_AND, KW_OR">
                <element ref-id="condition" />
            </iteration>
            <element ref-id="exists_condition" />
            <sequence id="000000255">
                <token id="000000256" type-id="CHR_LEFT_PARENTHESIS" />
                <one-of id="000000257">
                    <sequence id="000000258">
                        <element exit="true" ref-id="condition" />
                        <token id="000000259" type-id="CHR_RIGHT_PARENTHESIS" />
                    </sequence>
                    <sequence id="000000260">
                        <iteration id="000000261" separator="CHR_COMMA">
                            <element ref-id="expr" />
                        </iteration>
                        <token id="000000262" type-id="CHR_RIGHT_PARENTHESIS" />
                        <one-of id="000000263">
                            <sequence id="000000264">
                                <element ref-id="equality_operator" />
                                <wrapper id="000000265" template="PARENTHESES">
                                    <element ref-id="subquery" />
                                </wrapper>
                            </sequence>
                            <sequence id="000000266">
                                <token id="000000267" optional="true" type-id="KW_NOT" />
                                <token exit="true" id="000000268" type-id="KW_IN" />
                                <wrapper id="000000269" template="PARENTHESES">
                                    <one-of id="000000270" sortable="true">
                                        <element ref-id="expression_list" />
                                        <element ref-id="subquery" />
